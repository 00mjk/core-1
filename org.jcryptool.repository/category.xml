   <category-def name="org.jcryptool.core" label="JCrypTool Core">
      <description>
         JCrypTool Core contains all base plug-ins required to run JCrypTool.
      </description>
   </category-def>
   <category-def name="org.jcryptool.core.dependencies" label="JCrypTool Dependencies">
      <description>
         JCrypTool Core Dependencies contains all Eclipse plug-ins required by JCrypTool. The German language packs are optional but required for a fully internationalized version of JCrypTool.
      </description>
   </category-def>
   <category-def name="org.jcryptool.editors" label="JCrypTool Editors">
      <description>
         JCrypTool Editors contains all editors available for JCrypTool.
      </description>
   </category-def>
   <category-def name="org.jcryptool.providers" label="JCrypTool Providers">
      <description>
         JCrypTool Providers contains all crypto providers available for JCrypTool.
      </description>
   </category-def>
   <category-def name="org.jcryptool.crypto" label="JCrypTool Crypto">
      <description>
         JCrypTool Crypto contains all cryptography plug-ins - algorithms, analysis, games, visualizations and XML Security - available for JCrypTool.
      </description>
   </category-def>
