<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.jcryptool.core.action.opencmd"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+O"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.jcryptool.core.action.fileActions.OpenFile"
            id="org.jcryptool.core.action.opencmd"
            name="%command.openfile">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=newfile">
         <command
               commandId="org.jcryptool.core.action.opencmd"
               icon="icons/open_file.gif"
               label="%command.openfile"
               mnemonic="O"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
