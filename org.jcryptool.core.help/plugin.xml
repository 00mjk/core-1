<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.index">
      <index
            file="$nl$/index.xml">
      </index>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="org.jcryptool.core.cheatsheets.general"
            name="%cheatsheet.category.general">
      </category>
      <cheatsheet
            category="org.jcryptool.core.cheatsheets.general"
            contentFile="$nl$/cheatsheets/general.xml"
            id="org.jcryptool.core.cheatsheets.general.cheatsheet"
            name="%cheatsheet.general.name">
            <description>%cheatsheet.general.description</description>
      </cheatsheet>
   </extension>
   <extension
         point="org.eclipse.help.webapp.contentFilter">
      <filter
            class="org.jcryptool.core.help.JCTJSInjectionFilter"
            priority="1">
      </filter>
   </extension>

</plugin>
